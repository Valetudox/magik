{
  "name": "@magik/backend-socket-client",
  "version": "1.0.0",
  "description": "Generated TypeScript client for Backend Socket HTTP API (broadcast and health endpoints only)",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client.gen": "./src/client.gen.ts",
    "./types.gen": "./src/types.gen.ts",
    "./sdk.gen": "./src/sdk.gen.ts"
  },
  "scripts": {
    "generate": "openapi-ts -i ../../apps/backend-socket/openapi.yaml -o ./src -c @hey-api/client-fetch",
    "lint": "eslint src --max-warnings 0"
  },
  "peerDependencies": {
    "@hey-api/client-fetch": "^0.4.0"
  },
  "devDependencies": {
    "typescript": "~5.9.3"
  }
}
