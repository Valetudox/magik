{
  "name": "ui-audio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "bun run --filter='@magik/backend-audio-client' generate",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .vue,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.4.0",
    "@magik/backend-audio-client": "workspace:*",
    "@magik/ui-shared": "workspace:*",
    "@mdi/font": "^7.4.47",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "vuetify": "^3.10.11"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
